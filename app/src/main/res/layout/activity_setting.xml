<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setting">

    <ImageView
        android:id="@+id/background"
        android:layout_width="741dp"
        android:layout_height="501dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ghldnjsrkdlq" />

    <ImageView
        android:id="@+id/soundeffectimage"
        android:layout_width="80dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintEnd_toEndOf="@+id/background"
        app:layout_constraintHorizontal_bias="0.143"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.583"
        app:srcCompat="@drawable/anfqkddnf" />

    <ImageView
        android:id="@+id/pushNotification"
        android:layout_width="80dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintEnd_toEndOf="@+id/background"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.846"
        app:srcCompat="@drawable/anfqkddnf" />

    <TextView
        android:id="@+id/backbutton2"
        android:layout_width="95dp"
        android:layout_height="35dp"
        android:text="뒤로가기"
        android:fontFamily="@font/bazzi"
        android:textColor="#E44F72"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/backbutton"
        app:layout_constraintStart_toEndOf="@+id/backbutton"
        app:layout_constraintTop_toTopOf="@+id/backbutton" />

    <ImageView
        android:id="@+id/backbutton"
        android:layout_width="37dp"
        android:layout_height="41dp"
        android:layout_marginStart="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.1"
        app:srcCompat="@drawable/oranagearrow_left" />

    <ImageView
        android:id="@+id/backgroundsoundimage"
        android:layout_width="80dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintEnd_toEndOf="@+id/background"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.323"
        app:srcCompat="@drawable/anfqkddnf" />

    <TextView
        android:id="@+id/backgroundsoundtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="배경음"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/backgroundsoundimage"
        app:layout_constraintEnd_toEndOf="@+id/backgroundsoundimage"
        app:layout_constraintStart_toStartOf="@+id/backgroundsoundimage"
        app:layout_constraintTop_toTopOf="@+id/backgroundsoundimage" />

    <TextView
        android:id="@+id/soundeffecttext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="효과음"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/soundeffectimage"
        app:layout_constraintEnd_toEndOf="@+id/soundeffectimage"
        app:layout_constraintStart_toStartOf="@+id/soundeffectimage"
        app:layout_constraintTop_toTopOf="@+id/soundeffectimage" />

    <TextView
        android:id="@+id/pushNotificationtext"
        android:layout_width="63dp"
        android:layout_height="20dp"
        android:text="PUSH알림"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/pushNotification"
        app:layout_constraintEnd_toEndOf="@+id/pushNotification"
        app:layout_constraintStart_toStartOf="@+id/pushNotification"
        app:layout_constraintTop_toTopOf="@+id/pushNotification" />

    <ToggleButton
        android:id="@+id/backgroundsoundtoggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#7DD4FF"
        android:text="ToggleButton"
        android:textColor="#FFFFFF"
        android:textOff="끄기"
        android:textOn="켜기"
        android:fontFamily="@font/bazzi"
        app:layout_constraintBottom_toBottomOf="@+id/backgroundsoundimage"
        app:layout_constraintStart_toEndOf="@+id/backgroundsoundimage"
        app:layout_constraintTop_toTopOf="@+id/backgroundsoundimage" />

    <ToggleButton
        android:id="@+id/pushNotificationtoggleButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#7DD4FF"
        android:text="ToggleButton"
        android:textColor="#FFFFFF"
        android:textOff="끄기"
        android:textOn="켜기"
        android:fontFamily="@font/bazzi"
        app:layout_constraintBottom_toBottomOf="@+id/pushNotification"
        app:layout_constraintStart_toEndOf="@+id/pushNotification"
        app:layout_constraintTop_toTopOf="@+id/pushNotification" />

    <ToggleButton
        android:id="@+id/soundeffecttoggleButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#7DD4FF"
        android:text="ToggleButton"
        android:textColor="#FFFFFF"
        android:textOff="끄기"
        android:textOn="켜기"
        android:fontFamily="@font/bazzi"
        app:layout_constraintBottom_toBottomOf="@+id/soundeffectimage"
        app:layout_constraintStart_toEndOf="@+id/soundeffectimage"
        app:layout_constraintTop_toTopOf="@+id/soundeffectimage" />

    <ImageView
        android:id="@+id/fontchangeplate"
        android:layout_width="376dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintEnd_toEndOf="@+id/background"
        app:layout_constraintHorizontal_bias="0.89"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.792"
        app:srcCompat="@drawable/board_school" />

    <TextView
        android:id="@+id/fontchange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="248dp"
        android:text="폰트변경"
        android:fontFamily="@font/bazzi"
        android:textColor="#093447"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/fontchangeplate"
        app:layout_constraintEnd_toEndOf="@+id/fontchangeplate"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fontchangeplate" />

    <ImageView
        android:id="@+id/fontchangecheckimage"
        android:layout_width="58dp"
        android:layout_height="59dp"
        android:layout_marginBottom="248dp"
        app:layout_constraintBottom_toBottomOf="@+id/fontchangeplate"
        app:layout_constraintEnd_toEndOf="@+id/fontchangeplate"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/fontchangeplate"
        app:srcCompat="@drawable/word_menu_name_blank" />

    <TextView
        android:id="@+id/fontchangechecktext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확인"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/fontchangecheckimage"
        app:layout_constraintEnd_toEndOf="@+id/fontchangecheckimage"
        app:layout_constraintStart_toStartOf="@+id/fontchangecheckimage"
        app:layout_constraintTop_toTopOf="@+id/fontchangecheckimage"
        app:layout_constraintVertical_bias="0.65" />

    <TextView
        android:id="@+id/imagesizingtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이미지 크기 조절"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintEnd_toEndOf="@+id/background"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintStart_toStartOf="@+id/background"
        app:layout_constraintTop_toTopOf="@+id/background"
        app:layout_constraintVertical_bias="0.179" />

    <ImageView
        android:id="@+id/imagepercentage"
        android:layout_width="64dp"
        android:layout_height="58dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toStartOf="@+id/imagesizingtext"
        app:layout_constraintTop_toBottomOf="@+id/imagesizingtext"
        app:srcCompat="@drawable/gameicon_empty" />

    <TextView
        android:id="@+id/imagepercentageText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00%"
        android:fontFamily="@font/bazzi"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/imagepercentage"
        app:layout_constraintEnd_toEndOf="@+id/imagepercentage"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="@+id/imagepercentage"
        app:layout_constraintTop_toTopOf="@+id/imagepercentage"
        app:layout_constraintVertical_bias="0.794" />

</androidx.constraintlayout.widget.ConstraintLayout>