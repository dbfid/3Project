        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="학교"
        android:fontFamily="@font/bazzi"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/schoolimage"
